{
  "kind": "collectionType",
  "collectionName": "external_links",
  "info": {
    "singularName": "external-link",
    "pluralName": "external-links",
    "displayName": "External Link",
    "description": "External links associated with sections"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "teaser": {
      "type": "text"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "externalLinks"
    }
  }
}
